{
  "name": "aria-voyager",
  "version": "0.1.3",
  "description": "A framework agnostic / universal package that implements navigation patterns for various aria roles and features",
  "author": "gossi",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "imports": {
    "#src": "./src/index",
    "#src/*": "./src/*",
    "#tests/": "./tests/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "start": "tsdown --watch",
    "build": "tsdown",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@gossi/config-eslint": "1.3.1",
    "@gossi/config-prettier": "1.1.1",
    "@hokulea/core": "0.9.1",
    "@hokulea/theme-moana": "0.4.0",
    "@testing-library/dom": "10.4.1",
    "@types/css-modules": "1.0.5",
    "@types/eslint": "9.6.1",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-istanbul": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "es-toolkit": "^1.43.0",
    "eslint": "^9.38.0",
    "playwright": "^1.57.0",
    "prettier": "3.6.2",
    "tsdown": "0.19.0",
    "typescript": "5.8.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}
