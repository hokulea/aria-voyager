{
  "name": "svelte-aria-voyager",
  "version": "0.0.0",
  "description": "Navigation patterns for various aria roles and features in svelte",
  "author": "gossi",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --browser.headless --browser.provider=playwright",
    "test:ui": "vitest"
  },
  "dependencies": {
    "aria-voyager": "workspace:*",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@gossi/config-eslint": "0.14.0",
    "@gossi/config-prettier": "0.10.0",
    "@hokulea/core": "^0.3.1",
    "@hokulea/theme-moana": "^0.0.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/dom": "10.4.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/css-modules": "1.0.5",
    "@types/eslint": "9.6.1",
    "@types/lodash.isequal": "4.5.8",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "concurrently": "9.1.2",
    "eslint": "8.57.1",
    "playwright": "^1.51.1",
    "prettier": "3.6.2",
    "svelte": "^5.26.2",
    "typescript": "5.8.3",
    "vite": "^6.2.4",
    "vitest": "^3.1.1",
    "vitest-browser-svelte": "^0.1.0"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}
