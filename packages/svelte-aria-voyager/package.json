{
  "name": "svelte-aria-voyager",
  "version": "0.0.0",
  "description": "Navigation patterns for various aria roles and features in svelte",
  "author": "gossi",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --browser.provider=preview --no-browser.headless --browser.name=firefox"
  },
  "dependencies": {
    "aria-voyager": "workspace:*",
    "lodash.isequal": "^4.5.0"
  },
  "devDependencies": {
    "@gossi/config-eslint": "1.3.0",
    "@gossi/config-prettier": "1.1.0",
    "@hokulea/core": "^0.7.1",
    "@hokulea/theme-moana": "^0.2.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/dom": "10.4.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/css-modules": "1.0.5",
    "@types/lodash.isequal": "4.5.8",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "9.36.0",
    "playwright": "^1.55.1",
    "prettier": "3.6.2",
    "svelte": "^5.39.9",
    "typescript": "5.8.3",
    "vite": "^6.3.6",
    "vitest": "^3.1.1",
    "vitest-browser-svelte": "^1.1.0"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}
