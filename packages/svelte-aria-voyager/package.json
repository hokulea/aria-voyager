{
  "name": "svelte-aria-voyager",
  "private": true,
  "version": "0.0.0",
  "description": "Navigation patterns for various aria roles and features in svelte",
  "author": "gossi",
  "license": "MIT",
  "repository": "https://github.com/hokulea/aria-voyager",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --browser.provider=preview --no-browser.headless --browser.name=firefox"
  },
  "dependencies": {
    "aria-voyager": "workspace:*"
  },
  "devDependencies": {
    "@gossi/config-eslint": "catalog:lint",
    "@gossi/config-prettier": "catalog:lint",
    "@hokulea/core": "catalog:lib",
    "@hokulea/theme-moana": "catalog:lib",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@testing-library/dom": "catalog:test",
    "@tsconfig/svelte": "^5.0.6",
    "@vitest/browser-playwright": "catalog:test",
    "@vitest/coverage-istanbul": "catalog:test",
    "@vitest/ui": "catalog:test",
    "eslint": "catalog:lint",
    "es-toolkit": "catalog:lib",
    "playwright": "catalog:test",
    "prettier": "catalog:lint",
    "svelte": "^5.48.1",
    "typescript": "catalog:build",
    "vite": "catalog:build",
    "vitest": "catalog:test",
    "vitest-browser-svelte": "^2.0.1"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}
